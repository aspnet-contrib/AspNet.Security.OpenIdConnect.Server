{
  "buildOptions": {
    "emitEntryPoint": true,
    "warningsAsErrors": true,
    "preserveCompilationContext": true,

    "copyToOutput": {
      "include": [
        "Content",
        "Views",
        "App.config"
      ]
    },

    "embed": {
      "include": [ "Certificate.pfx" ]
    }
  },

  "dependencies": {
    "EntityFramework": "6.1.3",
    "Microsoft.AspNet.Razor": "3.2.3",
    "Microsoft.Extensions.Logging.Debug": "1.0.0",
    "Microsoft.Extensions.Logging.Console": "1.0.0",
    "Microsoft.NETCore.Platforms": "1.0.1",
    "Microsoft.Owin.Security.Cookies": "3.0.1",
    "Microsoft.Owin.Security.Jwt": "3.0.1",
    "Microsoft.Owin.Security.OAuth": "3.0.1",
    "Microsoft.Owin.SelfHost": "3.0.1",
    "Nancy.MSOwinSecurity": "1.0.1",
    "Nancy.Owin": "1.3.0",
    "Nancy.Viewengines.Razor": "1.3.0",
    "NWebsec.Owin": "1.0.0",
    "Owin.Security.OAuth.Introspection": "1.0.0-alpha3-final",
    "Owin.Security.OAuth.Validation": "1.0.0-alpha3-final",
    "Owin.Security.OpenIdConnect.Server": { "target": "project" }
  },

  "frameworks": {
    "net451": { }
  },

  "publishOptions": {
    "include": [
      "Content",
      "Views",
      "App.config"
    ]
  }
}
